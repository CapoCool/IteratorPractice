package IteratorPractice;

/* Dillan Poorman
 * 
 */

public class NumberList{
	
	
	private Number[] numbers = new Number[10];
	private int index = 0;
	
	
	public void addNumber(int number) {
		
		numbers[index] = new Number(number);
		index++;
	}
	
	public Number[] getNumbers() {
		return numbers;
	}
}
